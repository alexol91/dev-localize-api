{
  "name": "localization-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:dreamteammobile/telecoming/localization-api.git"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npm run copy-static",
    "copy-static": "cp -rv public dist/public",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "start:debug": "nodemon --config nodemon-debug.json",
    "postinstall": "npm run prestart:prod",
    "prestart:prod": "rimraf dist && npm run build && npm run copy-static",
    "start:prod": "node dist/src/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "lint:fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "test": "npm run test:unit && npm run test:e2e",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles",
    "test:unit": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@google/maps": "^0.5.5",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-fastify": "^6.6.7",
    "@nestjs/swagger": "^3.1.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.9.1",
    "d3-geo": "^1.11.6",
    "dotenv": "^8.0.0",
    "fastify-static": "^2.5.0",
    "fastify-swagger": "^2.4.0",
    "firebase": "^6.3.1",
    "firebase-admin": "^8.2.0",
    "iplocation": "^6.1.0",
    "mongoose": "^5.6.6",
    "nest-schedule": "^0.6.3",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "simplify-geojson": "^1.0.3",
    "swagger-ui-express": "^4.0.7"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/d3-geo": "^1.11.1",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.0",
    "@types/google__maps": "^0.5.5",
    "@types/jest": "^23.3.13",
    "@types/mongoose": "^5.5.12",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "concurrently": "^4.1.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.4.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3",
    "wait-on": "^3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
